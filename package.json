{
  "name": "atlas",
  "license": "MPL-2.0",
  "scripts": {
    "build": "npm run build-prod",
    "build-acc": "grunt && webpack --config webpack.acc.js",
    "build-dev": "grunt && webpack --config webpack.dev.js",
    "build-prod": "grunt && webpack --config webpack.prod.js",
    "build-test": "grunt && webpack --config webpack.acc.js",
    "clean": "npm run clean-cache && npm run clean-dirs",
    "clean-cache": "npm cache verify",
    "clean-dirs": "rimraf ./node_modules ./coverage ./coverage-legacy ./dist ./build ./pa11y/*.png",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --format=node_modules/eslint-formatter-pretty src modules test/cypress --ext js,jsx",
    "lint:style": "sass-lint -c src/.sass-lint.yml 'src/**/*.scss' --verbose --no-exit && sass-lint -c modules/.sass-lint.yml 'modules/**/*.scss' --verbose --no-exit",
    "preinstall": "npm cache verify",
    "start": "grunt && webpack-dev-server --config webpack.dev.js",
    "start-prod": "grunt && webpack-dev-server --config webpack.prod-api-dev.js",
    "test": "karma start && jest --config=./test/jest.unit.config.js",
    "test-e2e-aria": "babel-node ./test/pa11y.js",
    "test-e2e": "cross-env API_ROOT=https://acc.api.data.amsterdam.nl ./scripts/cypress-run.sh",
    "test-e2e-prod": "cross-env API_ROOT=https://api.data.amsterdam.nl ./scripts/cypress-run.sh",
    "test-e2e-prod-watch": "cross-env API_ROOT=https://api.data.amsterdam.nl ./scripts/cypress-watch.sh",
    "test-e2e-watch": "cross-env API_ROOT=https://acc.api.data.amsterdam.nl ./scripts/cypress-watch.sh",
    "test-eslint": "eslint ./modules/ ./src/ ./test/cypress/",
    "test-integration": "cross-env NODE_ENV=development jest --config=./test/jest.integration.config.js",
    "test-integration-watch": "cross-env NODE_ENV=development jest --config=./test/jest.integration.config.js --watch",
    "test-jest": "cross-env NODE_ENV=development jest --config=./test/jest.unit.config.js",
    "test-jest-watch": "cross-env NODE_ENV=development jest --config=./test/jest.unit.config.js --watch",
    "test-karma": "karma start",
    "test-karma-watch": "karma start --no-single-run",
    "test-lint": "npm run lint",
    "test-watch": "karma start --no-single-run & jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amsterdam/atlas.git"
  },
  "dependencies": {
    "angular": "~1.5.11",
    "angular-aria": "~1.5.11",
    "angular-i18n": "~1.5.11",
    "angular-sanitize": "~1.5.11",
    "angulartics-piwik": "^1.0.6",
    "babel-preset-react-app": "^3.1.0",
    "bbga_visualisatie_d3": "https://github.com/Amsterdam/bbga_visualisatie_d3.git#master",
    "cross-env": "5.2.0",
    "d3": "^3.5.17",
    "escape-string-regexp": "1.0.5",
    "isomorphic-fetch": "2.2.1",
    "leaflet": "^1.3.1",
    "leaflet-draw": "^1.0.0",
    "leaflet-rotatedmarker": "^0.2.0",
    "leaflet.markercluster": "^1.0.0",
    "leaflet.nontiledlayer": "^1.0.7",
    "lodash.debounce": "^4.0.8",
    "lodash.defer": "^4.1.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.identity": "^3.0.0",
    "lodash.isequal": "^4.5.0",
    "marked": "^0.3.6",
    "marzipano": "https://github.com/Amsterdam/marzipano.git#master",
    "objectFitPolyfill": "^2.1.0",
    "proj4": "^2.4.4",
    "proj4leaflet": "^1.0.2",
    "prop-types": "^15.6.0",
    "raven-js": "^3.19.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-leaflet": "^1.9.0",
    "react-leaflet-markercluster": "^1.1.8",
    "react-redux": "^5.0.6",
    "react-resize-aware": "^2.7.0",
    "recharts": "^1.0.0-beta.10",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "remove-markdown": "^0.2.2",
    "reselect": "^3.0.1"
